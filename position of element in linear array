#include <stdio.h>
int main() {
  int n, i, target, a[100];
  printf("Enter size of array: ");
  scanf("%d", &n)
  printf("Enter elements: ");
  for (i = 0; i < n; i++) scanf("%d", &a[i]);
  printf("Enter target: ");
  scanf("%d", &target);
  for (i = 0; i < n; i++) {
    if (a[i] == target) {
      printf("Found at position %d\n", i + 1);
      return 0;
    }
  }
  printf("Not found\n");
  return 0;
}
